<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
  <file original="" product-name="Wegmeister.TagManager" source-language="en" datatype="plaintext" target-language="de">
    <body>
      <trans-unit id="tagmanager.tags.title" xml:space="preserve" approved="yes">
        <source>Tags for group "{0}"</source>
        <target xml:lang="de">Tags der Gruppe "{0}"</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.noTags" xml:space="preserve" approved="yes">
        <source>No tags created yet.</source>
        <target xml:lang="de">Es wurden noch keine Tags erstellt.</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.createTag" xml:space="preserve" approved="yes">
        <source>Create new tag</source>
        <target xml:lang="de">Neuen Tag erstellen</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.editTag" xml:space="preserve" approved="yes">
        <source>Edit tag "{0}"</source>
        <target xml:lang="de">Tag "{0}" bearbeiten</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.deleteTag" xml:space="preserve" approved="yes">
        <source>Delete tag</source>
        <target xml:lang="de">Tag löschen</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.delete" xml:space="preserve" approved="yes">
        <source>Do you really want to delete the tag "{0}"? This operation cannot be undone!</source>
        <target xml:lang="de">Wollen Sie den Tag "{0}" wirklich löschen? Dies kann nicht rückgängig gemacht werden!</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.deleteModalConfirm" xml:space="preserve" approved="yes">
        <source>Yes, delete the tag</source>
        <target xml:lang="de">Ja, Tag wirklich löschen</target>
      </trans-unit>

      <trans-unit id="tagmanager.tags.newTag" xml:space="preserve" approved="yes">
        <source>New tag</source>
        <target xml:lang="de">Neuer Tag</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.newTagInfo" xml:space="preserve" approved="yes">
        <source>Just fill out the following form to create a new tag:</source>
        <target xml:lang="de">Tragen Sie den Namen des neuen Tags ein:</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.tagName" xml:space="preserve" approved="yes">
        <source>Tag name</source>
        <target xml:lang="de">Tag-Name</target>
      </trans-unit>
      <trans-unit id="tagmanager.tags.tagGroup" xml:space="preserve" approved="yes">
        <source>Tag group</source>
        <target xml:lang="de">Tag-Gruppe</target>
      </trans-unit>

      <trans-unit id="tagmanager.tags.showTag" xml:space="preserve" approved="yes">
        <source>Show tag "{0}"</source>
        <target xml:lang="de">Tag "{0}" anschauen</target>
      </trans-unit>

      <trans-unit id="tagmanager.tags.update" xml:space="preserve" approved="yes">
        <source>Save changes</source>
        <target xml:lang="de">Änderungen speichern</target>
      </trans-unit>


      <trans-unit id="tagmanager.groups.title" xml:space="preserve" approved="yes">
        <source>Groups</source>
        <target xml:lang="de">Gruppen</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.noGroups" xml:space="preserve" approved="yes">
        <source>No groups created yet.</source>
        <target xml:lang="de">Es wurden noch keine Gruppen erstellt.</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.groupName" xml:space="preserve" approved="yes">
        <source>Group name</source>
        <target xml:lang="de">Gruppenname</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.createGroup" xml:space="preserve" approved="yes">
        <source>Create new group</source>
        <target xml:lang="de">Neue Gruppe erstellen</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.editGroup" xml:space="preserve" approved="yes">
        <source>Edit group "{0}"</source>
        <target xml:lang="de">Gruppe "{0}" bearbeiten</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.deleteGroup" xml:space="preserve" approved="yes">
        <source>Delete group</source>
        <target xml:lang="de">Gruppe löschen</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.delete" xml:space="preserve" approved="yes">
        <source>Do you really want to delete the group "{0}"? This will delete the group and all the tags created in this group! This operation cannot be undone!</source>
        <target xml:lang="de">Wollen Sie die Gruppe "{0}" wirklich löschen? Dadurch wird die Gruppe und alle Tags, die zur Gruppe gehören, gelöscht! Dies kann nicht rückgängig gemacht werden!</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.deleteModalConfirm" xml:space="preserve" approved="yes">
        <source>Yes, delete the group</source>
        <target xml:lang="de">Ja, Gruppe wirklich löschen</target>
      </trans-unit>

      <trans-unit id="tagmanager.groups.newGroup" xml:space="preserve" approved="yes">
        <source>New group</source>
        <target xml:lang="de">Neue Gruppe</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.newGroupInfo" xml:space="preserve" approved="yes">
        <source>Just fill out the following form to create a new group:</source>
        <target xml:lang="de">Tragen Sie den Namen der neuen Gruppe ein:</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.groupName" xml:space="preserve" approved="yes">
        <source>Group name</source>
        <target xml:lang="de">Name der Gruppe</target>
      </trans-unit>
      <trans-unit id="tagmanager.groups.showGroup" xml:space="preserve" approved="yes">
        <source>Show group "{0}"</source>
        <target xml:lang="de">Gruppe "{0}" anschauen</target>
      </trans-unit>

      <trans-unit id="tagmanager.groups.update" xml:space="preserve" approved="yes">
        <source>Save changes</source>
        <target xml:lang="de">Änderungen speichern</target>
      </trans-unit>
    </body>
